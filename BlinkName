int led1 = D6; 

void setup() {

   
    pinMode(led1, OUTPUT);
  

}


void loop()
{
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(2000);

    
    digitalWrite(led1, LOW);
    delay(2000);
    
    digitalWrite(led1, HIGH);
    delay(2000);

    
    digitalWrite(led1, LOW);
    delay(2000);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);

    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, LOW);
    delay(2000);
    
    digitalWrite(led1, HIGH);
    delay(2000);
    
    digitalWrite(led1, LOW);
    delay(2000);
    
    digitalWrite(led1, HIGH);
    delay(2000);
    
    digitalWrite(led1, LOW);
    delay(2000);
    
    digitalWrite(led1, HIGH);
    delay(2000);
    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);
    
    digitalWrite(led1, LOW);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(500);
    
    digitalWrite(led1, HIGH);
    delay(2000);
    
    digitalWrite(led1, LOW);
    delay(2000);
    
    
}
